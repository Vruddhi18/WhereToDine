{
  "version": 2,
  "builds": [
    {
      "src": "Restar/app.py",
      "use": "@vercel/python"
    },
    {
      "src": "Restar/assets/js/server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "Restar/app.py"
    },
    {
      "src": "/server/(.*)",
      "dest": "Restar/assets/js/server.js"
    }
  ]
}