{
  "version": 2,
  "builds": [
    {
      "src": "src/server.js",
      "use": "@vercel/node",
      "config": {
        "includeFiles": ["src/**"]
      }
    },
    {
      "src": "api/app.py",
      "use": "@vercel/uvicorn app:app --reload"
    }
  ],
  "routes": [
    {
      "src": "/Restar/(.*)",
      "dest": "api/index.py"
    },
    {
      "src": "/(.*)",
      "dest": "src/server.js",
      "methods": ["GET", "POST", "PUT", "DELETE", "PATCH", "OPTIONS"]
    }
  ]
}
