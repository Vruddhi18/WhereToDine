{
  "version": 2,
  "builds": [
    {
      "src": "assets/js/server.js",
      "use": "@vercel/node",
      "config": {
        "includeFiles": ["src/**"]
      }
    },
    {
      "src": "/.",
      "use": "@vercel/uvicorn app:app --reload"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "app.py"
    },
    {
      "src": "assets/js/(.*)",
      "dest": "assets/js/server.js",
      "methods": ["GET", "POST", "PUT", "DELETE", "PATCH", "OPTIONS"]
    }
  ]
}
